{
  "name": "Childsafe",
  "version": "1.0",
  "description": "Surfing under your protection",
  "manifest_version": 2,
  "permissions": [
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "storage"
  ],
  "background": {
    "scripts": ["js/background.js"],
    "persistent": true
  },
  "icons": {
    "16": "images/child16.png",
    "32": "images/child32.png",
    "48": "images/child48.png",
    "128": "images/child128.png"
  }
}
